          <geom type="mesh" rgba="0.75294 0.75294 0.75294 1" mesh="right_link" contype="1" conaffinity="0"  group="1" class="visualgeom" />
          <geom type="mesh" rgba="0.75294 0.75294 0.75294 1" mesh="right_link" />
        </body>
      </body>
  </body>
